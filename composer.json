{
	"name": "oldskool/whoops-cakephp",
	"type": "cakephp-plugin",
	"description": "Integrates the Whoops library into a CakePHP 3.x application",
	"homepage": "https://github.com/oldskool/WhoopsCakephp",
	"license": "MIT",
	"version": "2.0.0",
	"require": {
		"composer/installers": "*",
		"cakephp/cakephp": "3.0.*-dev",
		"filp/whoops": "1.*"
	},
	"extra": {
		"installer-name": "WhoopsCakephp"
	},
	"autoload": {
		"psr-4": {
			"WhoopsCakephp\\": "src/"
		}
	}
}
